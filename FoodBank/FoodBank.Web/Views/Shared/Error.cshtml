@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_AccountLayout.cshtml";
}

<div class="content">


    <div class="container-fluid text-center">
        <h1 class="error-title">Error</h1>
        <h6 class="text-semibold content-group">Oops, an error has occurred.</h6>

        <div class="row">
            <div class="col-lg-4 col-lg-offset-4 col-sm-6 col-sm-offset-3">

                <div class="input-group content-group">

                </div>

                <div class="row">
                    <div class="col-sm-12">
                        <a href="#" class="btn btn-primary btn-block content-group"><i class="icon-circle-left2 position-left"></i> Go to dashboard</a>
                    </div>

                    @TempData["ErrorMessage"]
                    @TempData["ErrorMessage2"]
                </div>

            </div>
        </div>
    </div>
</div>

